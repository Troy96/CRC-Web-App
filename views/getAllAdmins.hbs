<br />
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <div class="well">
                {{#if admins}}
                <div class="table-responsive">
                    <table class="table" id="allAdmins">
                        <thead>
                            <tr>
                                <th scope="col">S.No.</th>
                                <th scope="col">Admin ID</th>
                                <th scope="col">First Name</th>
                                <th scope="col">Last Name</th>
                                <th scope="col">Email</th>
                                <th scope="col">Phone</th>
                                <th scope="col">Status</th>

                            </tr>
                        </thead>
                        <div id="div1">
                            <tbody id="tbody" name="tbody">

                                {{#each admins}}
                                <tr>
                                    <td>{{inc @index}}</td>
                                    <td>{{_id}}</span>
                                    <td>{{first_name}}</td>
                                    <td>{{last_name}}</td>
                                    <td>{{email}}</td>
                                    <td>{{phone}}</td>
                                    <td><button type="button" class="deleteAdmin" class="btn btn-danger">Delete</button></td>
                                </tr>
                                {{/each}}
                                {{else}}
                                <td>No admins to display!</td>
                                {{/if}}
                            </tbody>
                        </div>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>